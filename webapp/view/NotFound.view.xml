<mvc:View
	controllerName="my.sampleorders.SampleOrders.controller.SalesOrder"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">
	<MessagePage
		title="{i18n>notFoundTitle}"
		text="{i18n>notFoundText}"
		icon="{sap-icon://document}"
		id="page">
		<customDescription>
			<Link id="link" text="{i18n>backToOrders}" press="myNavBack" />
		</customDescription>
	</MessagePage>
</mvc:View>